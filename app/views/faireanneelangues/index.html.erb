<main class="col-sm-9 offset-sm-3 col-md-10 offset-md-2 pt-3">
  <p id="notice"><%= notice %></p>

  <h1>Faireanneelangues</h1>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @faireanneelangues.each do |faireanneelangue| %>
        <tr>
          <td><%= faireanneelangue.name %></td>
          <td><%= link_to 'Show', faireanneelangue %></td>
          <td><%= link_to 'Edit', edit_faireanneelangue_path(faireanneelangue) %></td>
          <td><%= link_to 'Destroy', faireanneelangue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Faireanneelangue', new_faireanneelangue_path %>
</main>